  name: shark-deployment
  labels:
    app: custom
spec:
  replicas: 2
  selector:
    matchLabels:
      app: custom
  template:
    metadata:
      labels:
        app: custom
    spec:
      containers:
      - name: shark
        image: lokeshloki1122/devsecopsfinaliage
        ports:
        - containerPort: 8080
